		<list id="300" name="accesibility_type">
			<label type="item">Accesibility type</label>
			<label type="list">Type</label>
			<items>
				<item id="301" >
				    <code>all_weather_road</code>
				    <label>All-Weather Road</label>
				</item>
				<item id="302">
						<code>seasonal_road</code>
						<label>Seasonal Road</label>
