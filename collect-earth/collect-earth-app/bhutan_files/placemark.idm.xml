			</code>
			<code id="12" name="site_bearing" list="bearing" relevant="land_use_category!='noData' and site_accessibility!='inaccesible'" >
				<label xml:lang="en">Bearing</label>
			</code>
			<text name="directions" id="13" required="false" relevant="false()">
			    <label xml:lang="en">Directions</label>
			</text>
			<entity id="14" name="topography" multiple="true" >
