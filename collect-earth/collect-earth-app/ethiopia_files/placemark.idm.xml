					<code>UNKNOWN</code>
					<label>UNKNOWN</label>
				</item>					
			</items>
		</list>
	</codeLists>
	<spatialReferenceSystems>
		<spatialReferenceSystem srid="EPSG:4326">
			<label xml:lang="en">WGS 84</label>
			<description xml:lang="en">WGS 84</description>
			<wkt><![CDATA[
	        GEOGCS["WGS 84",
		  		DATUM["WGS_1984",
		  			SPHEROID["WGS 84",6378137,298.257223563, AUTHORITY["EPSG","7030"]],
		        	AUTHORITY["EPSG","6326"]],
	    		PRIMEM["Greenwich",0,
	        		AUTHORITY["EPSG","8901"]],
	    		UNIT["degree",0.01745329251994328,
	        		AUTHORITY["EPSG","9122"]],
	    		AUTHORITY["EPSG","4326"]]
			]]></wkt>
		</spatialReferenceSystem>
	</spatialReferenceSystems>
	
	<schema>
		<entity id="1" name="plot" ui:tabSet="placemark" ui:tab="placemark">
			<label xml:lang="en">Plot</label>
			<text id="2" name="id" required="true" key="true">
				<label xml:lang="en">Id</label>
			</text>
			<coordinate id="3" name="location">
				<label xml:lang="en">Location</label>
			</coordinate>
			<text id="4" name="operator">
				<label xml:lang="en">Operator</label>
			</text>
			<entity id="5" name="land_use" multiple="true" minCount="1" >
			    <label xml:lang="en">Land Use</label>
				<code id="6" name="type" list="land_use" key="true">
					<label xml:lang="en">Type</label>
				</code>
				<number name="number_of_points" id="7" type="integer" required="false" relevant="false()">
				    <label xml:lang="en">Number of points</label>
				</number>
				<!-- <check flag="error" expr="sum(number_of_points)!=25" ><message>The sum of the number of points has to be equal to 25</message></check> -->
			</entity>			
			<code id="11" name="interpretation_uncertainty" list='uncertainty_type' >
				<label xml:lang="en">Uncertainty of interpretation</label>
			</code>
			<boolean id="12" name="presence_wetlands" >
			    <label xml:lang="en">Presence of wetlands</label>
			</boolean>
			<boolean id="13" name="presence_planted_forest" >
			    <label xml:lang="en">Presence of planted forest</label>
			</boolean>
			<boolean id="14" name="presence_woody_vegetation" >
			    <label xml:lang="en">Presence of woody vegetation</label>
			</boolean>
			<code id="16" name="topography_relief" list="relief_type" >
				<label xml:lang="en">Relief</label>
			</code>
			<entity id="17" name="accesibility" multiple="true" >
			    
			    <label xml:lang="en">Proximity to closest infrastructure</label>
				<code id="18" name="type" list="accesibility_type" key="true">
					<label xml:lang="en">Type</label>
				</code>

				<number name="distance" id="19" type="real"  required="false" relevant="false()">
				    <label xml:lang="en">Distance</label>
				</number>
				
				<!-- <check flag="error" expr="sum(distance)!=0"><message>There is no distance set for any type of infrastructure</message></check> -->
			</entity>
			<code id="20" name="access_difficulty" list="difficulty_type" >
				<label xml:lang="en">Access Difficulty</label>
			</code>
			
			<code id="21" name="inventory_difficulty" list="difficulty_type" >
				<label xml:lang="en">Inventory Difficulty</label>
			</code>
			
			<date id="22" name="rs_date">
				<label xml:lang="en">RS Date</label>
			</date>
			<code id="23" name="rs_satellite" list="rs_satellite">
				<label xml:lang="en">RS Satellite</label>
			</code>			
			<text name="notes" id="24" required="false" relevant="false()">
			    <label xml:lang="en">Notes</label>
			</text>
			<boolean id="25" name="actively_saved">
				<label xml:lang="en">Actively saved by user</label>
			</boolean>			
			<number id="30" name="elevation" type="integer" >
				<label xml:lang="en">Elevation</label>
			</number>
			<code id="31" name="orientation" list="orientation">
				<label xml:lang="en">Orientation</label>
			</code>
			<number id="32" name="slope_north_south" type="real" >
				<label xml:lang="en">Slope North South</label>
			</number>
			<number id="33" name="slope_west_east" type="real" >
				<label xml:lang="en">Slope West East</label>
			</number>
		</entity>
	</schema>
</survey>