	<schema>
		<entity id="1" name="plot" ui:tabSet="placemark" ui:tab="placemark">
			<label xml:lang="en">Plot</label>
			<text id="2" name="id" required="true" key="true">
				<label xml:lang="en">Id</label>
			</text>
			<coordinate id="3" name="location">
				<label xml:lang="en">Location</label>
			</coordinate>
			<text id="4" name="operator">
				<label xml:lang="en">Operator</label>
			</text>
			<code id="5" name="land_use" list="land_use">
				<label xml:lang="en">Land use</label>
			</code>
			<boolean id="6" name="land_use_change" relevant="land_use!='noData'">
				<label xml:lang="en">Land use development</label>
			</boolean>
			<code id="7" name="land_use_type" list="land_use" parent="land_use" relevant="land_use!='noData'">
				<label xml:lang="en">Land use type</label>
			</code>
			<boolean id="21" name="interpretation_uncertainty" relevant="land_use='forest' or land_use='grassland'" >
				<label xml:lang="en">Interpretation uncertainty</label>
			</boolean>
			<code id="8" name="topography_accessibility" list="topographical_accessibility" relevant="land_use!='noData'">
				<label xml:lang="en">Accessibility</label>
			</code>
			<entity id="20" name="topography" multiple="true" >
			    <label xml:lang="en">Topography</label>
				<code id="9" name="element" list="topographical_elements" key="true">
					<label xml:lang="en">Element</label>
				</code>
				<code id="10" name="coverage" list="topographical_elements_coverage">
					<label xml:lang="en">Coverage</label>
				</code>
			</entity>
			<code id="11" name="human_impact_type" list="human_impact_type"
				multiple="true" relevant="land_use!='settlement' and land_use!='noData'">
				<label xml:lang="en">Human Impact Type</label>
			</code>
			<code id="12" name="human_impact_grade" list="human_impact_grade" relevant="land_use!='noData' and land_use!='settlement' and human_impact_type!='na'">
				<label xml:lang="en">Human Impact Grade</label>
			</code>
			<number id="13" name="human_impact_year" type="integer" relevant="land_use!='noData' and land_use!='settlement' and human_impact_type!='na'">
				<label xml:lang="en">Human Impact Year</label>
			</number>
			<date id="14" name="rs_date" relevant="land_use!='noData'">
				<label xml:lang="en">RS Date</label>
			</date>
			<code id="15" name="rs_satellite" list="rs_satellite" relevant="land_use!='noData'">
				<label xml:lang="en">RS Satellite</label>
			</code>
			<code id="16" name="crown_cover" list="crown_cover" relevant="land_use='forest'" >
				<label xml:lang="en">Crown Cover</label>
			</code>
			<code id="19" name="deforestation_reason" list="deforestation_reason" relevant="land_use='forest' and crown_cover='0'" >
				<label xml:lang="en">Deforestation reason</label>
			</code>
			<code id="17" name="cover_type" list="cover_type" relevant="land_use='forest' and crown_cover!='0'">
				<label xml:lang="en">Crown Type</label>
			</code>
			<boolean id="18" name="actively_saved">
				<label xml:lang="en">Actively saved by user</label>
			</boolean>
			<number id="30" name="elevation" type="integer" >
				<label xml:lang="en">Elevation</label>
			</number>
			<code id="31" name="orientation" list="orientation">
				<label xml:lang="en">Orientation</label>
			</code>
			<number id="32" name="slope_north_south" type="real" >
				<label xml:lang="en">Slope North South</label>
			</number>
			<number id="33" name="slope_west_east" type="real" >
				<label xml:lang="en">Slope West East</label>
			</number>
		</entity>
	</schema>
