					</item>
					<item id="164">
						<code>dense_other_land</code>
						<label>Other Land - Dense Cover (&gt;=50%)</label>
					</item>
				
			</items>
		</list>

		<list id="200" name="rs_satellite">
			<label type="item">Satellite</label>
			<label type="list">Satellite/sensor</label>
			<items>
				<item id="201">
						<code>landsat</code>
						<label>Landsat</label>
				</item>
				<item id="202">
						<code>quickbird</code>
						<label>Quickbird</label>
				</item>
				<item id="203">
